            <oneOrMore>
               <group>
                  <choice>
                     <ref name="castItem"/>
          
                  </choice>
        
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
        
               </group>
            </oneOrMore>
            <zeroOrMore>
               <group>
        
                  <ref name="model.common"/>
        
        
                  <zeroOrMore>
                     <ref name="model.global"/>
                  </zeroOrMore>
        
               </group>
            </zeroOrMore>
         </group>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
